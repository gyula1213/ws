package hello;

public class Rnd {

	public static void main(String[] args) {
		int db=1000000;
		int []counter = new int [6];
		for( int i=0; i<db; i++ ) {
			double d = Math.random();
			int x = (int)(6*d);
			counter[x]++;
		}
		for( int i=0; i<6; i++ ) {
			System.out.println((i+1) + ": " + counter[i] + "  " + 100*counter[i]/db + "%");
		}
	}

}
